{
  "name": "line-awesome",
  "version": "1.2.2",
  "description": "Line Awesome by Icon8. A drop-in replacement for Font Awesome.",
  "keywords": [
    "font",
    "awesome",
    "icons",
    "icons8",
    "line-awesome",
    "font-awesome"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Airgrup/line-awesome.git"
  },
  "license": "MIT",
  "main": "dist/line-awesome/css/line-awesome.css",
  "devDependencies": {
    "sass": "^1.55.0"
  },
  "scripts": {
    "compile": "npx sass ./dist/line-awesome/scss/line-awesome.scss > ./dist/line-awesome/css/line-awesome.css",
    "compile:min": "npx sass ./dist/line-awesome/scss/line-awesome.scss --output-style compressed > ./dist/line-awesome/css/line-awesome.min.css"
  }
}
